{
  "env": {
    "role": {
        "guest": [10000],
        "host": [9999],
        "arbiter": [10000]
    },
    "ip_map": {
        "9999": "172.16.153.134",
        "10000": -1
    }
  },
  "tasks": {
    "poisson": {
      "data": [
        {
          "file": "examples/data/motor_mini_b.csv",
          "head": 1,
          "partition": 10,
          "table_name": "motor_mini_b",
          "namespace": "motor_mini",
          "role": "guest_0"
        },
        {
          "file": "examples/data/motor_mini_a.csv",
          "head": 1,
          "partition": 10,
          "table_name": "motor_mini_a",
          "namespace": "motor_mini",
          "role": "host_0"
        }
      ],
      "conf": "./test_hetero_poisson_train_job_conf.json",
      "dsl": "./test_hetero_poisson_train_job_dsl.json",
      "type": "train"
    },
    "poisson-predict": {
      "conf": "./test_predict_conf.json",
      "task": "poisson",
      "type": "predict"
    },
    "poisson-cv": {
      "conf": "./test_hetero_poisson_cv_job_conf.json",
      "dsl": "./test_hetero_poisson_cv_job_dsl.json",
      "task": "poisson",
      "type": "train"
    }
  }
}